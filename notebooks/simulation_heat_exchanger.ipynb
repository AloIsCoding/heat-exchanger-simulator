    "    \"helium\": 1.96e-5      # At 20°C\n",
    "}\n",
    "\n",
    "# Dictionary of thermal conductivity for fluids (W/m·K)\n",
    "thermal_conductivity_fluid = {\n",
    "    \"water\": 0.6,          # At 20°C\n",
    "    \"air\": 0.026,         # At 20°C\n",
    "    \"thermal oil\": 0.15,   # Approx.\n",
